{
  "name": "6ad",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "npm run db:generate && turbo run build",
    "start": "node apps/api/dist/index.js",
    "lint": "turbo run lint",
    "dev:api": "npm run dev --workspace=apps/api",
    "dev:admin": "npm run dev --workspace=apps/admin",
    "dev:agency": "npm run dev --workspace=apps/agency",
    "dev:user": "npm run dev --workspace=apps/user",
    "db:generate": "cd packages/database && npx prisma generate",
    "db:push": "cd packages/database && npx prisma db push",
    "db:studio": "cd packages/database && npx prisma studio",
    "postinstall": "npm run db:generate"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  }
}
